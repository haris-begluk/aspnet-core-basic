@model aspnet_core_basic.ViewModels.HomeIndexViewModels
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Home</title>
</head>
<body>
<table>  
    <h1>@Model.CurrentMessage</h1>
    @foreach(var item in Model.Restaurants) 
    {<tr> 
        <td>@item.Id</td> 
        <td>@item.Name</td> 
        <td><a href="/home/details/@item.Id">Details</a> 
      
        </td>  
        <td>
        @Html.ActionLink("GO", "Details",new{id=item.Id}) 
        </td> 
        <td><a asp-action="Details" asp-route-id="@item.Id">More</a></td>
    </tr>}
    
</table>
</body>
</html>